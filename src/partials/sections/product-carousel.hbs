<!-- product carousel section -->
<section id="product-carousel-ab-original" class="product-carousel pt-4 pb-4 {{ class }}">
  {{#unless noTabs}}
  <h2 class="h1 text-center mb-1 mb-lg-2">Our Products</h2>
  <ul class="nav nav-tabs mx-auto nav-tabs--product text-center mb-0 justify-content-center border-0 pb-4 pb-lg-2" role="tablist">
    <li class="nav-item">
      <a class="nav-link border-0 text-decoration-none h4 mb-0 font-weight-normal" data-toggle="tab" href="#new" role="tab"
        aria-controls="new">New</a>
    </li>
    <li class="nav-item">
      <a class="nav-link border-0 text-decoration-none h4 mb-0 font-weight-normal active" data-toggle="tab" href="#bestsellers"
        role="tab" aria-controls="bestsellers"><span>Bestsellers</span><span class="d-none">Featured</span></a>
    </li>
    <li class="nav-item">
      <a class="nav-link border-0 text-decoration-none h4 mb-0 font-weight-normal" data-toggle="tab" href="#value-sets"
        role="tab" aria-controls="value-sets">Value Sets</a>
    </li>
  </ul>
  {{/unless}}
  <div class="tab-content container px-sm-0 px-md-2 text-center">
    <div class="tab-pane fade carousel slide carousel--loop" id="new" role="tabpanel" aria-labelledby="new-tab"
      data-slide-number="4">
      <div class="carousel--centered">
        <div class="carousel-inner row flex-nowrap mx-0" role="listbox">
          {{> components/product-card-carousel active="active" title="Sunny Honey Bali Bronzing Bundle" compare_price="$44.90" price="$34.90" carousel=true badge="New" }}
          {{> components/product-card-carousel title="Bronzing Face Drops" compare_price="$44.90" price="$34.90" swatch="true" swatch_shade="true" carousel=true square_badge="New" }}
          {{> components/product-card-carousel title="Sunny Honey Bali Bronzing Foam" compare_price="$44.90" price="$34.90" swatch="true" swatch_style="true" carousel=true }}
          {{> components/product-card-carousel title="Like A Virgin Hair Masque" compare_price="$44.90" price="$34.90" carousel=true }}
          {{> components/product-card-carousel title="Sunny Honey Bali Bronzing Bundle" compare_price="$44.90" price="$34.90" carousel=true }}
          {{> components/product-card-carousel title="Sunny Honey Bali Bronzing Foam" compare_price="$44.90" price="$34.90" carousel=true }}
          {{> components/product-card-carousel title="Like A Virgin Hair Masque" compare_price="$44.90" price="$34.90" carousel=true }}
        </div>
      </div>
      <a class="d-none d-lg-flex carousel-control carousel-control-prev carousel-control--background floating-out-left justify-content-start text-primary w-auto"
        href="#new" role="button" data-slide="prev">
        <span class="carousel-control-prev-icon d-flex justify-content-center align-items-center text-body bg-transparent" aria-hidden="true"><img class="svg" src="icons/chevron-prev.svg" replace-to-svg /></span>
        <span class="sr-only">Previous</span>
      </a>
      <a class="d-none d-lg-flex carousel-control carousel-control-next carousel-control--background floating-out-right justify-content-end text-primary w-auto"
        href="#new" role="button" data-slide="next">
        <span class="carousel-control-next-icon d-flex justify-content-center align-items-center text-body bg-transparent" aria-hidden="true"><img class="svg" src="icons/chevron-next.svg" replace-to-svg /></span>
        <span class="sr-only">Next</span>
      </a>
    </div>
    <div class="tab-pane fade show active carousel slide carousel--loop tab-abtest" id="bestsellers" role="tabpanel"
      aria-labelledby="bestsellers-tab" data-slide-number="4">
      <div class="carousel--centered">
        <div class="carousel-inner row flex-nowrap mx-0" role="listbox">
          {{> components/product-card-carousel active="active" title="Sunny Honey Bali Bronzing Bundle" compare_price="$44.90" price="$34.90" carousel=true }}
          {{> components/product-card-carousel title="Bronzing Face Drops" compare_price="$44.90" price="$34.90" swatch="true" swatch_style="true" badge="true" badge_image="badge-25.svg" carousel=true }}
          {{> components/product-card-carousel title="Sunny Honey Bali Bronzing Foam" compare_price="$144.90" price="$134.90" swatch="true" swatch_style="true" badge="true" badge_image="badge-25.svg" carousel=true }}
          {{> components/product-card-carousel title="Shampooing et Après-Shampooing Super Hydratants" compare_price="$44.90" price="$34.90" carousel=true square_badge="New" }}
          {{> components/product-card-carousel title="Sunny Honey Bali Bronzing Bundle" compare_price="$44.90" price="$34.90" carousel=true }}
          {{> components/product-card-carousel title="Bronzing Face Drops" compare_price="$44.90" price="$34.90" swatch="true" swatch_style="true" badge="true" badge_image="badge-25.svg" carousel=true }}
          {{> components/product-card-carousel title="Sunny Honey Bali Bronzing Foam" compare_price="$144.90" price="$134.90" carousel=true }}
          {{> components/product-card-carousel title="Shampooing et Après-Shampooing Super Hydratants" compare_price="$44.90" price="$34.90" carousel=true }}
        </div>
        <!--
        <div class="carousel-inner row flex-nowrap d-none" role="listbox">
          {{> components/product-card-carousel active="active" title="Like a Virgin Hair Masque" compare_price="$44.90" price="$34.90" carousel=true }}
          {{> components/product-card-carousel title="Sunny Honey Bronzing Bundle" compare_price="$44.90" price="$34.90" swatch="true" swatch_modal="true" carousel=true }}
          {{> components/product-card-carousel title="Like a Virgin (AB Test)" compare_price="$44.90" price="$34.90" swatch="true" swatch_style="true" badge="true" badge_image="badge-25.svg" carousel=true }}
          {{> components/product-card-carousel title="Like a Virgin Hair Masque" compare_price="$44.90" price="$34.90" carousel=true }}
          {{> components/product-card-carousel title="Like a Virgin Hair Masque" compare_price="$44.90" price="$34.90" carousel=true }}
          {{> components/product-card-carousel title="Like a Virgin Hair Masque" compare_price="$44.90" price="$34.90" carousel=true }}
          {{> components/product-card-carousel title="Like a Virgin Hair Masque" compare_price="$44.90" price="$34.90" carousel=true }}
          {{> components/product-card-carousel title="Like a Virgin Hair Masque" compare_price="$44.90" price="$34.90" carousel=true }}
        </div>
        -->
      </div>
      <a class="d-none d-lg-flex carousel-control carousel-control-prev carousel-control--background floating-out-left justify-content-start text-primary w-auto"
        href="#bestsellers" role="button" data-slide="prev">
        <span class="carousel-control-prev-icon d-flex justify-content-center align-items-center text-body bg-transparent" aria-hidden="true"><img class="svg" src="icons/chevron-prev.svg" replace-to-svg /></span>
        <span class="sr-only">Previous</span>
      </a>
      <a class="d-none d-lg-flex carousel-control carousel-control-next carousel-control--background floating-out-right justify-content-end text-primary w-auto"
        href="#bestsellers" role="button" data-slide="next">
        <span class="carousel-control-next-icon d-flex justify-content-center align-items-center text-body bg-transparent" aria-hidden="true"><img class="svg" src="icons/chevron-next.svg" replace-to-svg /></span>
        <span class="sr-only">Next</span>
      </a>
    </div>
    <div class="tab-pane fade carousel slide carousel--loop" data-slide-number="4" id="value-sets" role="tabpanel"
      aria-labelledby="value-sets-tab">
      <div class="carousel--centered">
        <div class="carousel-inner row flex-nowrap mx-0" role="listbox">
          {{> components/product-card-carousel active="active" title="Sunny Honey Bali Bronzing Bundle" compare_price="$44.90" price="$34.90" carousel=true }}
          {{> components/product-card-carousel title="Bronzing Face Drops" compare_price="$44.90" price="$34.90" swatch="true" swatch_shade="true" carousel=true }}
          {{> components/product-card-carousel title="Sunny Honey Bali Bronzing Foam" compare_price="$44.90" price="$34.90" swatch="true" swatch_style="true" carousel=true }}
