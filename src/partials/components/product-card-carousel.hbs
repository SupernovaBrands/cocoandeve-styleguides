<div class="{{#if carousel }}carousel-item {{else}} d-flex flex-column {{/if}} {{#if carousel }} col-9 col-md-3 {{else}}col-6 col-md-4 mb-5 {{/if}} {{active}} product-card text-center">
	<a href="#">
	<noscript class="loading-lazy">
		<picture class="embed-responsive embed-responsive-1by1 bg-shimmer m-0">
			<img class="embed-responsive-item fit--cover"
			alt="Image Alt"
			loading="lazy"
			src="https://via.placeholder.com/243x243">
		</picture>
	</noscript>
	{{#if badge }}
		<picture class="circle-badge position-absolute rounded-circle bg-primary d-flex p-1">
			<img class="w-100" src="../images/{{ badge_image }}"/>
		</picture>
	{{/if}}
	</a>
	<div class="pt-2 pb-0 position-relative flex-grow-1 d-flex flex-column {{#if collection }} px-lg-2 {{else}} px-2 {{/if}}">
		<div class="d-flex justify-content-center">
			<div class="react-yotpo-star" data-product-id="4543113265187"></div>
		</div>
		<p class="product-card__title flex-grow-1 d-flex flex-column justify-content-center h4 h-100 font-weight-normal">
			<a href="#" class="text-dark text-decoration-none">{{title}}</a>
		</p>
		<p class="text-center">
			{{#if compare_price }}<span class="text-linethrough h4 m-1">{{compare_price}}</span>{{/if}}<span class="text-primary h4">{{price}}</span>
		</p>
		{{#if swatch }}
		{{#if swatch_shade }}
		<button type="button" class="btn btn-lg btn-primary btn-choose btn-block px-0">Choose Shade</button>
		<div class="swatch-overlay flex-column align-items-center justify-content-end w-100 pb-0 position-absolute {{#if collection }} px-lg-2 {{else}} px-2 {{/if}}">
			<div class="text-center w-100 pt-2 pb-lg-2">
				<label class="mb-2"><strong>Shade: </strong><span data-swatch-label>Dark</span></label>
				<ul class="list-unstyled product-variant-swatch d-flex justify-content-center">
					<li class="product-variant-swatch__item available col-3" data-available="available">
						<span data-id="32068891541539" data-val="Medium" class="d-block variant-swatch shade-medium"></span>
					</li>
					<li class="product-variant-swatch__item available active col-3" data-available="available">
						<span data-id="32068891607075" data-val="Dark" class="d-block border-primary mx-auto variant-swatch shade-dark"></span>
					</li>
					<li class="product-variant-swatch__item available col-3" data-available="available">
						<span data-id="32068891639843" data-val="Ultra Dark" class="d-block mx-auto  variant-swatch shade-ultra-dark"></span>
					</li>
				</ul>
			</div>
			<button type="button" class="btn btn-lg btn-primary btn-block add-to-cart">
				Add To Cart
			</button>
		</div>
		{{/if}}

		{{#if swatch_style }}
		<button type="button" class="btn btn-lg btn-primary btn-choose btn-block px-0">Choose Style</button>
		<div class="swatch-overlay flex-column align-items-center justify-content-end w-100 pb-0 position-absolute {{#if collection }} px-lg-2 {{else}} px-2 {{/if}}">
			<div class="text-center w-100 pt-2 pb-lg-2">
				<label class="mb-2"><strong>Style: </strong><span data-swatch-label>Girl Print: Limited edition!</span></label>
				<ul class="list-unstyled product-variant-swatch d-flex justify-content-center">
					<li class="product-variant-swatch__item available active col-3" data-available="available">
						<span data-id="32068891607075" data-val="Girl Print" class="d-block border-primary mx-auto  variant-swatch girl-print"></span>
					</li>
					<li class="product-variant-swatch__item available col-3" data-available="available">
						<span data-id="32068891639843" data-val="Print Leaf" class="d-block mx-auto  variant-swatch leaf-print"></span>
					</li>
				</ul>
			</div>
			<button type="button" class="btn btn-lg btn-primary add-to-cart btn-block px-0">
				Add To Cart
			</button>
		</div>
		{{/if}}
		{{#if swatch_style_eyemask }}
		<button type="button" class="btn btn-lg btn-primary btn-choose btn-block px-0">Choose Style</button>
		<div class="swatch-overlay flex-column align-items-center justify-content-end w-100 pb-0 position-absolute {{#if collection }} px-lg-2 {{else}} px-2 {{/if}}">
			<div class="text-center w-100 pt-2 pb-lg-2">
				<label class="mb-2"><strong>Style: </strong><span data-swatch-label>Girl Print: Limited edition!</span></label>
				<ul class="list-unstyled product-variant-swatch d-flex justify-content-center">
					<li class="product-variant-swatch__item available active col-3" data-available="available">
						<span data-id="32068891607075" data-val="Girl Print" class="d-block border-primary mx-auto  variant-swatch pink"></span>
					</li>
					<li class="product-variant-swatch__item available col-3" data-available="available">
						<span data-id="32068891639843" data-val="Print Leaf" class="d-block mx-auto  variant-swatch green"></span>
					</li>
				</ul>
			</div>
			<button type="button" class="btn btn-lg btn-primary add-to-cart btn-block px-0">
				Add To Cart
			</button>
		</div>
		{{/if}}
		{{#if swatch_scent }}
		<button type="button" class="btn btn-lg btn-primary btn-choose btn-block px-0">Choose Scent</button>
		<div class="swatch-overlay flex-column align-items-center justify-content-end w-100 pb-0 position-absolute {{#if collection }} px-lg-2 {{else}} px-2 {{/if}}">
			<div class="text-center w-100 pt-2 pb-lg-2">
				<label class="mb-2"><strong>Scent: </strong><span data-swatch-label>Girl Print: Limited edition!</span></label>
				<ul class="list-unstyled product-variant-swatch d-flex justify-content-center">
					<li class="product-variant-swatch__item available active col-3" data-available="available">
						<span data-id="32068891607075" data-val="Girl Print" class="d-block border-primary mx-auto  variant-swatch sweet-manggo"></span>
					</li>
					<li class="product-variant-swatch__item available col-3" data-available="available">
						<span data-id="32068891639843" data-val="Print Leaf" class="d-block mx-auto  variant-swatch lychee-dragonfruit"></span>
					</li>
				</ul>
			</div>
			<button type="button" class="btn btn-lg btn-primary add-to-cart btn-block px-0">
				Add To Cart
			</button>
		</div>
		{{/if}}
		{{ else }}
		<button type="button" class="btn btn-lg btn-primary add-to-cart btn-block px-0">
			Add To Cart
		</button>
		{{/if}}
	</div>
</div>
