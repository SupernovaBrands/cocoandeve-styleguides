      <form class="my-3 row align-items-center justify-content-center">
        <p class="h4 font-weight-normal mr-3 mb-0">Filter By:</p>
        <select id="real-result__select" class="custom-select col-5 d-lg-none">
          <option value="#all" selected="selected">All</option>
          <option value="#tan">Tan</option>
          <option value="#hair">Hair</option>
          <option value="#body">Body</option>
        </select>
        <ul id="real-result__main-tab" class="nav nav-tabs nav-tabs--real-results text-center d-none d-lg-flex" role="tablist">
          <li class="nav-item" role="presentation">
            <a class="nav-link text-decoration-none h4 mb-0 font-weight-normal" data-toggle="tab" href="#tan" role="tab">Tan</a>
          </li>
          <li class="nav-item" role="presentation">
            <a class="nav-link text-decoration-none h4 mb-0 font-weight-normal" data-toggle="tab" href="#hair" role="tab">Hair</a>
          </li>
          <li class="nav-item" role="presentation">
            <a class="nav-link text-decoration-none h4 mb-0 font-weight-normal" data-toggle="tab" href="#body" role="tab">Body</a>
          </li>
          <li class="nav-item" role="presentation">
            <a class="nav-link text-decoration-none active h4 mb-0" data-toggle="tab" href="#all" role="tab">All</a>
          </li>
        </ul>
