  {{> sections/testimonials-carousel}}
  {{> sections/product-carousel}}
  {{> sections/instagram bgclass="bg-yellow-light"}}
  {{> sections/services}}
  {{> sections/footer}}
	{{> js jquery=true bootstrap=true main=true }}
